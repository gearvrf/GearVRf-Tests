    <Script DEF='ControlxRotate'>
        <field accessType='inputOnly' name='rotation' type='SFRotation'/>
        <field accessType='inputOutput' name='xRotation' type='SFRotation'/>

        <![CDATA[
            ecmascript:
                function rotation (rotValue, valueNone)
                {
                    xRotation.setValue( rotValue, 1, 0, 0 );
                }
        ]]>
    </Script>
            
    <ROUTE fromField='rotation_changed' fromNode='xRotateCYLSENSOR' toField='set_rotation' toNode='ControlxRotate'/>
