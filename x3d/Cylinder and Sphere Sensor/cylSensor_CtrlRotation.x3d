    <ROUTE fromField='xRotation' fromNode='ControlxRotate' toField='rotation' toNode='tranX'/>
    

    <Script DEF='ControlyRotate'>
        <field accessType='inputOnly' name='rotation' type='SFRotation'/>
        <field accessType='inputOutput' name='yRotation' type='SFRotation'/>

